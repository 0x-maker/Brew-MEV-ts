    "@solana/wallet-adapter-base": "^0.9.24",
    "@solana/web3.js": "^1.87.6",
    "@types/crypto-js": "^4.2.2",
    "@types/open": "^6.2.1",
    "axios": "^1.6.8",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.1",
